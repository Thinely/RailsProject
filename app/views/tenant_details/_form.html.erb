  <div class="field form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :cid %>
    <%= form.number_field :cid, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :village %>
    <%= form.text_field :village, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :gewog %>
    <%= form.text_field :gewog, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :dzongkhag %>
    <%= form.text_field :dzongkhag, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :total_family_memeber %>
    <%= form.number_field :total_family_memeber, class:"form-control col-6" %>
  </div>

  <div class="field form-group">
    <%= form.label :phone_no %>
    <%= form.number_field :phone_no, class:"form-control col-6" %>
  </div>
  <input type="submit" class="btn btn-primary" value="Submit">

