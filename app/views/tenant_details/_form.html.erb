<%= form_with(model: tenant_detail) do |form| %>
  <% if tenant_detail.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(tenant_detail.errors.count, "error") %> prohibited this tenant_detail from being saved:</h2>

      <ul>
        <% tenant_detail.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field form-group">
    <%= form.label :user_id %>
    <%= form.collection_select(:user_id, @user_options,:id,:email,  { class: 'form-control col-6' }) %>

